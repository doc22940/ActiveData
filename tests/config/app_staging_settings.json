{
	"use": "elasticsearch",
	"testing":{
		"query": "http://127.0.0.1:5000/query",
		"sql": "http://127.0.0.1:5000/sql"
	},
	"flask": {
		"host": "0.0.0.0",
		"port": 5000,
		"debug": false,
		"threaded": true,
		"processes": 1,
		"ssl_context": "adhoc",
		"allow_exit": true
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://wiki.mozilla.org/Auto-tools/Projects/ActiveData"
		},
		"pyLibrary.env.big_data.MAX_STRING_SIZE": 100000000,
		"jx_elasticsearch.meta.ENABLE_META_SCAN": true,
		"jx_elasticsearch.meta.DEBUG": true,
		"jx_elasticsearch.es52.setop.DEBUG": true,
		"jx_elasticsearch.es52.aggs.DEBUG": true
	},
	"request_logs": {
		"host": "http://localhost",
		"port": 9200,
		"index": "activedata_requests",
		"type": "request_log",
		"typed": false,
		"schema": {
			"$ref": "//.../resources/schema/request_log.schema.json"
		}
	},
	"saved_queries":{
		"host": "http://localhost",
		"port": 9200,
		"index": "saved_queries",
		"type": "query",
		"debug": true
	},
	"elasticsearch": {
		"host": "http://localhost",
		"port": 9201,
		"index": "testdata",
		"type": "test_result",
		"debug": true
	},
	"debug": {
		"trace": true,
		"cprofile": {
			"enabled": false,
			"filename": "./results/profile.tab"
		},
		"log": [
			{
				"log_type": "console"
			},
			{
				"log_type": "ses",
				"from_address": "klahnakoski@mozilla.com",
				"to_address": "klahnakoski@mozilla.com",
				"subject": "[ALERT][DEV] Problem in ActiveData Frontend6",
				"$ref": "file://~/private.json#aws_credentials"
			},
			{
				"log_type": "elasticsearch",
				"host": "http://localhost",
				"port": 9200,
				"index": "debug_active_data",
				"type": "active_data"
			},
			{
				"class": "logging.handlers.RotatingFileHandler",
				"filename": "/data1/logs/activedata.log",
				"maxBytes": 10000000,
				"backupCount": 10,
				"encoding": "utf8"
			}
		]
	}

}
